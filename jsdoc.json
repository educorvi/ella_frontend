{
  "tags": {
    "allowUnknownTags": ["optional", "category", "subcategory"]
  },
  "source": {
    "include": ["src", "config/ella.config.template.js"],
    "includePattern": "\\.(vue|js|mjs)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-vuejs",
    "node_modules/better-docs/category"
  ],
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "better-docs": {
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/educorvi/ella_frontend"
        }
      ],
      "css": "docstyles.css",
      "name": "Ella Documentation",
      "hideGenerator": true
    }
  }
}
